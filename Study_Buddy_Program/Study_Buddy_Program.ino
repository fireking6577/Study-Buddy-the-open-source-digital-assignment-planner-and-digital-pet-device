//Class Initialization
Adafruit_SH1106G display = Adafruit_SH1106G(128, 64, &Wire, -1);

//Main Methods
//----------------------------------------------------------------------------------------------------------------------------------------------------------------
void setup() 
{
  //Initialize SPI devices
  Serial.begin(9600);
  Wire.begin(21, 22);
  SD.begin();
  display.begin(0x3c, true); 

  //Initialize input pins
  pinMode(25 , INPUT);
  pinMode(26 , INPUT);
  pinMode(27 , INPUT);
  pinMode(32 , INPUT);
  pinMode(33 , INPUT);

  //General setup
  display.setTextSize(1);
  display.setTextColor(SH110X_WHITE);
  pullData();
  updateDisplay();

  if(isDead == false)
  {
    xTaskCreate(
    beginPetClock,       
    "Pet Clock Task",    
    1000,                
    NULL,      
    1,                
    NULL              
    );
  }
}

void loop() 
{
  checkPins();
  //OK
  if (pin1 == 1)
  {
    if (tasks[lineSelectLocation - 1] == "<>")
    {
      createTask();
    }
    else
    {
      removeTask();
    }
    delay(200);
  }
  checkPins();
  //EXIT
  if (pin2 == 1)
  {
    //No code here
    delay(200);
  }
  checkPins();
  //UP
  if (pin3 == 1)
  {
    lineSelectLocation--;
    if (lineSelectLocation < 1)
    {
      lineSelectLocation = 1;
    }
    updateDisplay();
    delay(200);
  }
  checkPins();
  //DOWN
  if (pin4 == 1)
  {
    lineSelectLocation++;
    if (lineSelectLocation > 25)
    {
      lineSelectLocation = 25;
    }
    updateDisplay();
    delay(200);
  }
  checkPins();
  //MODE
  if (pin5 == 1)
  {
    goToPet();
    delay(200);
  }
}

//Other Helper Methods
//----------------------------------------------------------------------------------------------------------------------------------------------------------------
void createTask()
{
  String currentTaskName = "";
  char selectionMenu[] = {'a', 'b', 'c', 'd', 'e', 'f', 'g', 'h', 'i', 'j', 'k', 'l', 'm', 'n', 'o', 'p', 'q', 'r', 's', 't', 'u', 'v', 'w', 'x', 'y', 'z', '1', '2', '3', '4', '5', '6', '7', '8', '9', '0', '_', '<'};
  int selectedCharacterIndex = 0;
  updateCreateMenu(currentTaskName , selectionMenu[selectedCharacterIndex]);
  delay(200);
  while (true)
  {
    checkPins();
    //OK
    if (pin1 == 1)
    {
      if (selectionMenu[selectedCharacterIndex] == '<')
      {
        currentTaskName = currentTaskName.substring(0 , currentTaskName.length() -1);
      }
      else if (selectionMenu[selectedCharacterIndex] == '_')
      {
        currentTaskName += ' ';
      }
      else
      {
        currentTaskName += selectionMenu[selectedCharacterIndex];
      }
      updateCreateMenu(currentTaskName , selectionMenu[selectedCharacterIndex]);
      highlightChar();
      delay(200);
      highlightChar();
    }
    checkPins();
    //EXIT
    if (pin2 == 1)
    {
      break;
    }
    checkPins();
    //UP
    if (pin3 == 1)
    {
      selectedCharacterIndex--;
      if (selectedCharacterIndex < 0)
      {
        selectedCharacterIndex = 37;
      }
      updateCreateMenu(currentTaskName , selectionMenu[selectedCharacterIndex]);
      delay(200);
      
    }
    checkPins();
    //DOWN
    if (pin4 == 1)
    {
      selectedCharacterIndex++;
      if (selectedCharacterIndex > 37)
      {
        selectedCharacterIndex = 0;
      }
      updateCreateMenu(currentTaskName , selectionMenu[selectedCharacterIndex]);
      delay(200);
    }
    checkPins();
    //MODE
    if (pin5 == 1)
    {
      if (currentTaskName == "")
      {
        currentTaskName = "<>";
      }
      tasks[lineSelectLocation -1] = currentTaskName;
      pushData();
      break;
    }
  }
  updateDisplay();
}

void removeTask()
{
  display.clearDisplay();
  display.setCursor(0 , 0);
  display.println("Would you like to    complete this task?");
  display.drawLine(0, 20, 128, 20, SH110X_WHITE);
  display.setCursor(0 , 25);
  display.println(tasks[lineSelectLocation -1]);
  display.drawLine(0, 48, 128, 48, SH110X_WHITE);
  display.setCursor(0 , 52);
  display.println("OK: Yes | EXIT: No");
  display.display();
  delay(200);
  while (true)
  {
    checkPins();
    if(pin1 == 1)
    {
      tasks[lineSelectLocation-1] = "<>";
      amountOfFood++;
      display.clearDisplay();
      display.setTextSize(2);
      display.setCursor(6 , 20);
      display.println(" +1 Food");
      display.display();
      delay(600);
      display.setTextSize(1);
      pushData();
      break;
    }
    if(pin2 == 1)
    {
      break;
    }
  }
  updateDisplay();
}

void highlightLine(int line)
{
  line = (line - 1) * 8;
  for (int i = 0; i < 128; i++)
  {
    for (int j = line-1; j < line + 8; j++)
    {
      bool pixel = display.getPixel(i, j);
      display.drawPixel(i, j, !pixel);
    }
  }
  display.display(); 
}

void highlightChar()
{
  for (int i = 70; i < 79; i++)
  {
    for (int j = 51; j < 63; j++)
    {
      bool pixel = display.getPixel(i, j);
      display.drawPixel(i, j, !pixel);
    }
  }
  display.display(); 
}

void printLineToDisplay(String message)
{
  if (message == "<>") 
  {
    message = "<None>";
  }

  if (message.length() > 16) 
  {
    message = message.substring(0, 12) + "(...)";
  }

  String formattedMessage = (linePrintLocation < 10) ? "0" + String(linePrintLocation) + ") " + message : String(linePrintLocation) + ") " + message;
  display.println(formattedMessage);
  
  linePrintLocation++;
  if (linePrintLocation > 25) 
  {
    linePrintLocation = 1; 
  }
  display.display();
}

void pullData()
{
  File file = SD.open("/PlannerData.txt");
  if (!file)
  {
    display.clearDisplay();
    display.setCursor(0 , 0);
    display.println("<ERROR>");
    display.println("");
    display.println("No SD card found..!  please insert a      correctly formatted  SD card to continue.");
    display.display();
    delay(300);
    esp_restart();
  }

  //load data
  for (int i = 0; i < 25; i++)
  {
    tasks[i] = file.readStringUntil('\n');
    tasks[i].trim();
  }
  String health = (file.readStringUntil('\n'));
  hunger = health.toInt();
  String food = (file.readStringUntil('\n'));
  amountOfFood = food.toInt();
  isDead = (file.readStringUntil('\n') == "true");
  file.close();
  return;
}

void pushData()
{
  if (SD.exists("/PlannerData.txt")) 
  {
    SD.remove("/PlannerData.txt");
  }
  File file = SD.open("/PlannerData.txt", FILE_WRITE);
  if (!file)
  {
    display.clearDisplay();
    display.setCursor(0 , 0);
    display.println("Error Autosaving..!  please insert a      correctly formatted  SD Card");
    display.display();
    delay(500);
   while (true)
    {
      if (pin1 == 1)
      {
        pushData();
        return;
      }
    }
  }
  for (int i = 0; i < 25; i++)
  {
    file.println(tasks[i]);
  }
  file.println(String(hunger));
  file.println(String(amountOfFood));
  file.println(isDead ? "true" : "false");
  file.close();
  return;
}

void updateDisplay()
{
  display.setCursor(0, 0);
  display.clearDisplay();

  int startLine = 0; 
  int endLine = 8;   

  if (lineSelectLocation > 4 && lineSelectLocation <= 21)
  {
    startLine = lineSelectLocation - 4; 
  }
  else if (lineSelectLocation > 21)
  {
    startLine = 17; 
  }

  endLine = startLine + 8; 
  linePrintLocation = startLine + 1; 

  for (int i = startLine; i < endLine; i++)
  {
    printLineToDisplay(tasks[i]);
  }

  // Highlight the currently selected line
  int highlightPosition = lineSelectLocation - startLine;
  highlightLine(highlightPosition);

  display.display();
}

void updateCreateMenu(String currentWord , char currentIndex)
{
  display.clearDisplay();
  display.setCursor(0 , 0);
  display.println("    Creating Task");
  display.drawLine(0, 15, 128, 15, SH110X_WHITE); 
  display.setCursor(0 , 20);
  display.println(currentWord);
  display.drawLine(0, 48, 128, 48, SH110X_WHITE); 
  display.setCursor(0 , 54);
  display.println("Character: |" + String(currentIndex) + "|");
  display.display();
}

void checkPins()
{
  pin1 = digitalRead(25);
  pin2 = digitalRead(26);
  pin3 = digitalRead(27);
  pin4 = digitalRead(32);
  pin5 = digitalRead(33);
}

//Digital pet methods
//----------------------------------------------------------------------------------------------------------------------------------------------------------------
void goToPet()
{
  if(isDead)
  {
    display.clearDisplay();
    printPetMenu();
    display.drawBitmap(0, 0, deadFrame, 128, 64, SH110X_WHITE);
    display.display();
    delay(200);
    while(true)
    {
      checkPins();
      //MODE
      if (pin5 == 1)
      {
        updateDisplay();
        return;
      }
    }
  }
  else
  {
    unsigned long previousTime = 0;
    const unsigned long frameTimeInterval = 300;
    printPetMenu();
    printPetFrame();
    delay(300);
    while(true)
    {
      unsigned long currentTime = millis();
      if (currentTime - previousTime >= frameTimeInterval)
      {
        printPetFrame();
        previousTime = currentTime;
      }
      checkPins();
      //MODE
      if (pin5 == 1)
      {
        updateDisplay();
        return;
      }
      checkPins();
      //OK
      if (pin1 == 1)
      {
        amountOfFood--;
        if (amountOfFood < 0)
        {
          amountOfFood = 0;
        }
        hunger += 10;
        if (hunger > 100)
        {
          hunger = 100;
        }
        pushData():
        //eat animation
        delay(300);
      }
      checkPins();
      //UP
      if (pin3 == 1)
      {
        //play animation
        delay(200);
      }
      checkPins();
      //DOWN
      if (pin4 == 1)
      {
        //pet animation
        delay(200);
      }
    }
  }
}

void printPetMenu()
{
  if (!isDead)
  {
    display.setCursor(0, 0);
    display.println("Full: " + String(hunger) + "%");
    display.println("Food: " + String(amountOfFood));
    display.setCursor(0 , 27);
    display.println("<BUTTONS>");
    display.println("OK: Feed");
    display.println("UP: Play");
    display.println("DOWN: Pet");
    display.drawLine(64, 0, 64, 63, SH110X_WHITE);
    display.drawLine(0, 20, 63, 20, SH110X_WHITE);
  }
  else
  {
    display.setCursor(0, 0);
    display.println("Your pet");
    display.println("is dead!");
    display.setCursor(0 , 27);
    display.println("X_X");
    display.drawLine(64, 0, 64, 63, SH110X_WHITE);
    display.drawLine(0, 20, 63, 20, SH110X_WHITE);
  }
  display.display();
}

void printPetFrame()
{
  static int hungryFrame = 1;
  static int unconsciousFrame = 1;
  static int happyFrame = 1;
  static int idleFrame = 1;
  static bool movingForward = true;
  
  display.clearDisplay();
  printPetMenu();

  //unconscious frame sequence
  if (hunger <= 20)
  {
    if (unconsciousFrame == 1)
    {
      display.drawBitmap(0, 0, unconsciousFrame1, 128, 64, SH110X_WHITE);
    }
    if (unconsciousFrame == 2)
    {
      display.drawBitmap(0, 0, unconsciousFrame2, 128, 64, SH110X_WHITE);
    }
    if (unconsciousFrame == 3)
    {
      display.drawBitmap(0, 0, unconsciousFrame3, 128, 64, SH110X_WHITE);
    }
    if (movingForward)
    {
      unconsciousFrame++;
      if (unconsciousFrame > 3)  
      {
        unconsciousFrame -= 2;  
        movingForward = false; 
      }
    }
    else
    {
      unconsciousFrame--;
      if (unconsciousFrame < 1) 
      {
        unconsciousFrame += 2;  
        movingForward = true;
      }
    }
  }

  //hungry frame sequence
  else if (hunger <= 40)
  {
    if (hungryFrame == 1)
    {
      display.drawBitmap(0, 0, hungryFrame1, 128, 64, SH110X_WHITE);
    }
    if (hungryFrame == 2)
    {
      display.drawBitmap(0, 0, hungryFrame2, 128, 64, SH110X_WHITE);
    }
    if (movingForward)
    {
      unconsciousFrame++;
      if (hungryFrame > 2)  
      {
        hungryFrame -= 2;  
        movingForward = false; 
      }
    }
    else
    {
      hungryFrame--;
      if (hungryFrame < 1) 
      {
        hungryFrame += 2;  
        movingForward = true;
      }
    }
  }

  //happy frame sequence 
  else if (hunger >= 85)
  {
    if (happyFrame == 1)
    {
      display.drawBitmap(0, 0, happyFrame1, 128, 64, SH110X_WHITE);
    }
    if (happyFrame == 2)
    {
      display.drawBitmap(0, 0, happyFrame2, 128, 64, SH110X_WHITE);
    }
    if (movingForward)
    {
      happyFrame++;
      if (happyFrame > 2)  
      {
        happyFrame -= 2;  
        movingForward = false; 
      }
    }
    else
    {
      happyFrame--;
      if (happyFrame < 1) 
      {
        happyFrame += 2;  
        movingForward = true;
      }
    }
  }

  //idle frame sequence
  else
  {
    if (idleFrame == 1)
    {
      display.drawBitmap(0, 0, idleFrame1, 128, 64, SH110X_WHITE);
    }
    if (idleFrame == 2)
    {
      display.drawBitmap(0, 0, idleFrame2, 128, 64, SH110X_WHITE);
    }
    if (idleFrame == 3)
    {
      display.drawBitmap(0, 0, idleFrame3, 128, 64, SH110X_WHITE);
    }
    if (movingForward)
    {
      idleFrame++;
      if (idleFrame > 3)  
      {
        idleFrame -= 2;  
        movingForward = false; 
      }
    }
    
    else
    {
      idleFrame--;
      if (idleFrame < 1) 
      {
        idleFrame += 2;  
        movingForward = true;
      }
    }
  }
  display.display();
}

void beginPetClock(void *parameter)
{
  for(;;)
  {
    vTaskDelay(300000 / portTICK_PERIOD_MS);
    hunger--;
    if(hunger <= 0)
    {
      isDead = true;
    }
    pushData():
  }
}


